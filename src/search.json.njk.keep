---
permalink: /search.json
eleventyExcludeFromCollections: true
---

{% set books = [] %}
{% for book in collections.all %}
  {% if book.inputPath.indexOf('/books/') != -1 and book.data.date_read %}
    {% set _ = books.push({
      "title": book.data.title,
      "author": book.data.author,
      "tags": book.data.tags,
      "url": book.url
    }) %}
  {% endif %}
{% endfor %}

{{ books | dump }}
